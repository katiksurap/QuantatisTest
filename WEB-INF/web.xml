<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>SessionCheckingfilter</filter-name>
        <filter-class>session.filters.SessionCheckingfilter</filter-class>
    </filter>
    <servlet>
        <servlet-name>sameetbackend</servlet-name>
        <servlet-class>com.logic.sameetbackend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sameetbackend</servlet-name>
        <url-pattern>/sameetbackend</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>path</param-name>
        <param-value>/opt/tunes</param-value>
    </context-param>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.logic.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createuser</servlet-name>
        <servlet-class>com.logic.createuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>com.logic.ChangePassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createuser</servlet-name>
        <url-pattern>/createuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>CRUDController</servlet-name>
    <servlet-class>com.programmingfree.controller.CRUDController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CRUDController</servlet-name>
    <url-pattern>/CRUDController</url-pattern>
  </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>frameset.jsp</welcome-file>
    </welcome-file-list>
</web-app>
